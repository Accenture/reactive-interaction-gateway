          },
          {
            "id": "put",
            "path": "/api",
            "method": "PUT",
            "secured": false
          },
          {
            "id": "patch",
            "path": "/api",
            "method": "PATCH",
            "secured": false
          },
          {
            "id": "delete",
            "path": "/api",
            "method": "DELETE",
            "secured": false
          },
          {
            "id": "head",
            "path": "/api",
            "method": "HEAD",
            "secured": false
          },
          {
            "id": "options",
            "path": "/api",
            "method": "OPTIONS",
            "secured": false
