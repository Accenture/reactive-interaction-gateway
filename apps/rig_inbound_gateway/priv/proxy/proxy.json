[
  {
    "id": "flight-information-service",
    "name": "flight-information-service",
    "auth_type": "jwt",
    "auth": {
      "use_header": true,
      "header_name": "Authorization",
      "use_query": false,
      "query_name": ""
    },
    "versioned": false,
    "version_data": {
      "default": {
        "endpoints": [
          {
            "id": "get-flights",
            "path": "/api/v1/fis/flights",
            "method": "GET",
            "not_secured": true
          },
          {
            "id": "put-flights",
            "path": "/api/v1/fis/flights",
            "method": "PUT",
            "not_secured": true
          },
          {
            "id": "get-flight-id",
            "path": "/api/v1/fis/flights/{id}",
            "method": "GET",
            "not_secured": true
          }
        ]
      }
    },
    "proxy": {
      "use_env": true,
      "target_url": "FIS_HOST",
      "port": 4040
    }
  },
  {
